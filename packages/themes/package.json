{
  "name": "@pesto-ui/themes",
  "version": "0.0.1",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./themes.css": "./dist/themes.css"
  },
  "scripts": {
